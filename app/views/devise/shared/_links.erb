<%-if devise_mapping.registerable? && controller_name != 'registrations' %>
  <% # link_to "Зарегестрироваться", new_registration_path(resource_name) %>
<% end -%>
<%- if devise_mapping.recoverable? && controller_name != 'passwords' %>
  <%= link_to "Забыли пароль?", new_password_path(resource_name) %><br><% end -%><%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to "Не получили инстуркций по активации аккаунта?", new_confirmation_path(resource_name) %><br>
<% end -%>
<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "Не получили инструкций по разблокировке?", new_unlock_path(resource_name) %><br>
<% end -%>
<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Войти с помошью: #{provider.to_s.titleize}", omniauth_authorize_path(resource_name, provider) %><br><% end -%><% end -%>